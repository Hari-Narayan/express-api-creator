{
  "name": "express-api-creator",
  "version": "0.1.3",
  "description": "Scaffold an Express.js boilerplate project. You can generate express.js boilerplate code with a single command.",
  "main": "index.js",
  "type": "module",
  "bin": {
    "express-api-creator": "./bin/setup.js"
  },
  "scripts": {
    "create": "node bin/setup create template=js",
    "mcr": "node bin/setup mcr=category:name,images,stock,description,price"
  },
  "keywords": [
    "cli",
    "node",
    "dotenv",
    "express",
    "mongodb",
    "mongoose",
    "scaffold",
    "generator",
    "javascript",
    "typescript",
    "boilerplate",
    "dotenv-expand"
  ],
  "author": "Hari Narayan",
  "license": "MIT",
  "dependencies": {
    "console-log-colors": "^0.5.0",
    "inquirer": "^12.5.2",
    "simple-git": "^3.27.0"
  }
}
